{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
  "no-unused-vars": "off", 
  "@typescript-eslint/no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
}

}
